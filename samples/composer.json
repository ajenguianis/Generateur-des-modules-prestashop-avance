{
  "name": "$companyNameLower/$moduleName",
  "description": "$companyName module $companyNameLower",
  "type": "prestashop-module",
  "authors": [
    {
      "name": "$companyNameLower",
      "email": "$contact_email"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "prepend-autoloader": false
  },
  "require": {
    "php": ">=7.2.0"
  },
  "autoload": {
    "psr-4": {
      "EvoGroup\\Module\\$nameSpace\\": "src/"
    },
    "classmap": ["$moduleName.php", "src/"],
    "exclude-from-classmap": []
  }
}
