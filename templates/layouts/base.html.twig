<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Generateur des modules{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            <!--     Fonts and icons     -->
            <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
            <link href="{{ asset('assets/plugins/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('assets/plugins/css/material-bootstrap-wizard.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('assets/plugins/css/select2.min.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('assets/custom/css/form.css') }}" rel="stylesheet" type="text/css"/>
            <link href="{{ asset('assets/custom/css/app.css') }}" rel="stylesheet" type="text/css"/>
{#            {{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            <script src="{{ asset('assets/plugins/js/jquery-2.2.4.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('assets/plugins/js/bootstrap.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('assets/plugins/js/jquery.bootstrap.js') }}" type="text/javascript"></script>
            <script src="{{ asset('assets/plugins/js/jquery.validate.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('assets/plugins/js/material-bootstrap-wizard.js') }}" type="text/javascript"></script>
            <script src="{{ asset('assets/plugins/js/select2.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('assets/custom/js/app.js') }}" type="text/javascript"></script>
{#            {{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
